<div class="container">
    <div class="row top10">
        <div class="col col-xs-10 col-xs-offset-1">
            <%
            if (comandas.length != 0) {
                %>
                <div class="row">
                    <%
                    for (produto of comandas[0].produtos) {
                        %>
                        <div class="col col-xs-6 col-md-3">
                            <div class="panel">
                                <div class="panel-body">
                                    <img class="img-responsive center-block" src="img/asahi.png">

                                    <div class="pull-left">
                                        <%- produto.nome %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <%
                    }
                    %>
                </div>
            <%
            } else {
                %>
                <div class="row">
                    <h3 class="text-center">Nenhuma comanda em aberto ainda!</h3>
                </div>
                <%
            }
            %>
        </div>
    </div>
</div>