<div class="container">
    <div class="row">
        <div class="col col-xs-10 col-xs-offset-1">
            <input type="text" class="form-control" placeholder="Pesquisar Produto...">
            
            <div class="row">
                
                <%
                for (produto of produtos) {
                    %>
                    <div class="col col-xs-6 col-md-3">
                        <div class="panel">
                            <div class="panel-body">
                                <img class="img-responsive center-block" src="img/asahi.jpg">
                                <%- produto.nome %>
                            </div>
                        </div>
                    </div>
                    <%
                }
                %>

            </div>
        </div>
    </div>
</div>